-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Bundle-Name: io.openliberty.xmlWS.3.0.internal.tools
Bundle-Description: IBM XML Web Services Tools, version ${bVersion}
Bundle-SymbolicName: io.openliberty.xmlWS.3.0.internal.tools

Export-Package: com.sun.tools.ws.ant;version="3.0", \
 com.ibm.jtc.jax.tools.ws;version=1.0.0, \
 com.ibm.jtc.jax.tools.ws.spi;version=1.0.0, \
 com.ibm.jtc.jax.xml.ws.api;version=1.0.0, \
 com.ibm.jtc.jax.xml.ws.policy;version=1.0.0, \
 com.ibm.ws.jaxws.tools;version="3.0", \
 org.jvnet.staxex;version="1.7.7", \
 com.sun.tools.xjc.*;version="3.0", \
 com.sun.xml.messaging.saaj.*;version="2.0", \
 org.jvnet.staxex.util;version="1.7.7"

Import-Package: \
 org.glassfish.jaxb.*;version="3.0", \
 jakarta.xml.soap.*;version="2.0", \
 jakarta.xml.bind.*;version="3.0", \
 *

Private-Package:\
 com.ibm.ws.jaxws.tools.*, \
 com.sun.tools.ws.wscompile;-split-package:=merge-last;version="3.0.0"

-includeresource:\
 @${repo;com.sun.xml.ws:jaxws-rt;3.0.0}!/!META-INF/maven/*, \
 @${repo;com.sun.xml.ws:jaxws-tools;3.0.0}!/!META-INF/maven/*, \
 @${repo;com.sun.org.apache.xml.internal:resolver;20050927}!/!META-INF/maven/*, \
 @${repo;org.jvnet.staxex:stax-ex;1.7.7}!/!META-INF/maven/*, \
 @${repo;com.sun.xml.stream.buffer:streambuffer;1.5.3}!/!META-INF/maven/*, \
 com/sun/tools/ws/wscompile=${bin}/com/sun/tools/ws/wscompile

instrument.disabled: true

-buildpath: \
    com.ibm.ws.jaxws.tools.2.2.10;version=latest,\
    io.openliberty.com.sun.xml.messaging.saaj;version=latest,\
    org.glassfish.jaxb:codemodel;version=3.0.0,\
    org.glassfish.jaxb:jaxb-core;version=3.0.0,\
    org.glassfish.jaxb:jaxb-jxc;version=3.0.0,\
    org.glassfish.jaxb:jaxb-runtime;version=3.0.0,\
    org.glassfish.jaxb:jaxb-xjc;version=3.0.0,\
    org.glassfish.jaxb:txw2;version=3.0.0,\
    org.glassfish.jaxb:xsom;version=3.0.0,\
    jakarta.jws:jakarta.jws-api;version=3.0.0,\
	io.openliberty.jakarta.xmlBinding.3.0;version=latest,\
	io.openliberty.xmlBinding.3.0.internal.tools;version=latest,\
	io.openliberty.jakarta.xmlWS.3.0;version=latest,\
	io.openliberty.jakarta.jws.3.0;version=latest,\
	io.openliberty.jakarta.soap.2.0;version=latest,\
	com.sun.xml.ws:jaxws-rt;version=3.0.0, \
	com.sun.xml.ws:jaxws-tools;version=3.0.0, \
	com.sun.org.apache.xml.internal:resolver;version=20050927, \
	org.jvnet.staxex:stax-ex;version=1.7.7, \
	com.sun.xml.stream.buffer:streambuffer;version=1.5.3, \
	org.glassfish.jaxb:jaxb-core;version=3.0.0,\
	org.glassfish.jaxb:jaxb-xjc;version=3.0.0,\
	com.ibm.ws.org.apache.ant;version=latest