-include= ~../ant_build/resources/bnd/bundle.props
-donotcopy=.jazzignore
bVersion=1.0

# For more information, see http://was.pok.ibm.com/xwiki/bin/view/Liberty/UsingBnd

Bundle-Name: JCA 1.6 BVT resource adapters bundle
Bundle-SymbolicName: test.jca-1.6.bvt.adapter
Bundle-Description: Test bundle for resource adapters; version=${bVersion}

# optional... 
WS-TraceGroup: test

Export-Package: \
    test.jca.workcontext

Import-Package: \
    !*.internal.*, *

Private-Package: \
    test.jca*

-dsannotations=test.jca.workcontext.CollectionContextProvider, test.jca.cfreference.CFReference
-metatypeannotations=test.jca.cfreference.Config
