-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Bundle-Name: JAX-RS 2.X Monitor
Bundle-SymbolicName: com.ibm.ws.jaxrs.2.x.monitor
Bundle-Description: IBM JAX-RS 2.x monitor; version=${bVersion}


WS-TraceGroup: JAXRS

Import-Package: \
    com.ibm.websphere.servlet.container, \
    javax.servlet; version="[2.6,3)", \
    !*.internal.*,*

Export-Package: \
    com.ibm.ws.jaxrs.monitor
    
Private-Package: \
	com.ibm.websphere.jaxrs

Include-Resource: \
    com/ibm/ws/jaxrs/pmi/servletStats.xml=resources/com/ibm/ws/jaxrs/pmi/servletStats.xml, \
    com/ibm/ws/jaxrs/pmi/resourceMethodStats.xml=resources/com/ibm/ws/jaxrs/pmi/resourceMethodStats.xml


DynamicImport-Package: com.ibm.websphere.monitor.meters;version="1.0.0", \
 com.ibm.websphere.monitor.annotation;version="1.0.0", \
 com.ibm.websphere.monitor.jmx;version="1.0.0", \
 com.ibm.ws.pmi.server;version="[1.1,2)", \
 com.ibm.ws.pmi.stat;version="[1.1,2)", \
 com.ibm.websphere.pmi;version="[1.1,2)", \
 com.ibm.wsspi.pmi.factory;version="[1.1,2)", \
 com.ibm.wsspi.pmi.stat;version="[1.1,2)" 
 
Liberty-Monitoring-Components: com.ibm.ws.jaxrs.monitor.JaxRsMonitor

instrument.disabled: true

-dsannotations:  \
 	com.ibm.ws.jaxrs.monitor.JaxRsMonitorProviderRegister

-buildpath: \
	com.ibm.ws.monitor,\
	com.ibm.ws.webcontainer,\
	com.ibm.websphere.appserver.spi.logging,\
	com.ibm.websphere.javaee.servlet.3.0;version=latest,\
	com.ibm.ws.container.service;version=latest,\
	com.ibm.ws.org.osgi.annotation.versioning;version=latest, \
	com.ibm.websphere.javaee.jaxrs.2.0,\
	com.ibm.websphere.org.osgi.core,\
	com.ibm.websphere.org.osgi.service.component,\
	com.ibm.wsspi.org.osgi.service.component.annotations,\
	com.ibm.websphere.javaee.cdi.1.0,\
	com.ibm.ws.jaxrs.2.0.common
