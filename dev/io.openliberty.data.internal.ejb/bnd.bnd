-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

javac.source: 17
javac.target: 17

Require-Capability: osgi.ee; filter:="(&(osgi.ee=JavaSE)(version=17))"

Bundle-Name: Jakarta Data integration with Enterprise Beans
Bundle-SymbolicName: io.openliberty.data.internal.ejb
Bundle-Description: Enables Jakarta Enterprise Beans to use Jakarta Data; version=${bVersion}

WS-TraceGroup: data

Import-Package: *

Private-Package: \
  io.openliberty.data.internal.ejb.*

IBM-Default-Config: OSGI-INF/wlp/defaultInstances.xml

Include-Resource:\
  OSGI-INF=resources/OSGI-INF

-dsannotations: io.openliberty.data.internal.ejb.EJBModuleTracker

-buildpath: \
  com.ibm.websphere.appserver.spi.artifact,\
  com.ibm.websphere.appserver.spi.logging,\
  com.ibm.websphere.appserver.spi.kernel.service;version=latest,\
  com.ibm.websphere.org.osgi.core,\
  com.ibm.websphere.org.osgi.service.component,\
  com.ibm.wsspi.org.osgi.service.component.annotations,\
  com.ibm.ws.container.service,\
  com.ibm.ws.ejbcontainer.core,\
  com.ibm.ws.org.osgi.annotation.versioning;version=latest,\
  io.openliberty.data.internal
