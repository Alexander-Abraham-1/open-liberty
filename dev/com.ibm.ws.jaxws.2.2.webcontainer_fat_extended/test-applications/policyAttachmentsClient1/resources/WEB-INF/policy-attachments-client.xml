<attachments
	xmlns:wsu="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-utility-1.0.xsd"
	xmlns:wsp="http://www.w3.org/ns/ws-policy"
	xmlns:wsa="http://www.w3.org/2005/08/addressing"
	xmlns:wsam="http://www.w3.org/2007/05/addressing/metadata">

	<wsp:PolicyAttachment wsdlNamespace="http://service1.policyattachments.ws.ibm.com/">
		<wsp:AppliesTo>
			<wsp:URI>http://service1.policyattachments.ws.ibm.com/HelloService/#wsdl11.service(HelloService)</wsp:URI>
		</wsp:AppliesTo>
		
		<wsp:Policy wsu:Id="WSAddressing1">	
			<wsp:ExactlyOne>
        		<wsp:All>
            		<wsam:Addressing>
   						<wsp:Policy xmlns:wsu="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-utility-1.0.xsd" wsu:Id="NonAnonymousOnlyEndpoint">
   							<wsam:Addressing>
   									<wsp:Policy>
   									<wsam:NonAnonymousResponses required="true"/>/>
   								</wsp:Policy>
   							</wsam:Addressing>
   						</wsp:Policy>
           			 </wsam:Addressing>
        		</wsp:All>
    		</wsp:ExactlyOne>
    	</wsp:Policy>
	</wsp:PolicyAttachment>
	
	<wsp:PolicyAttachment wsdlNamespace="http://service1.policyattachments.ws.ibm.com/">
		<wsp:AppliesTo>
			<wsp:URI>http://service1.policyattachments.ws.ibm.com/HelloService2/#wsdl11.service(HelloService2)</wsp:URI>	
		</wsp:AppliesTo>	
		<wsp:Policy wsu:Id="WSAddressing2">
			<wsp:ExactlyOne>
        		<wsp:All>
            		<wsam:Addressing>
   						<wsp:Policy xmlns:wsu="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-utility-1.0.xsd" wsu:Id="AnonymousOnlyEndpoint">
   							<wsam:Addressing>
   								<wsp:Policy>
   									<wsam:AnonymousResponses required="true"/>/>
   								</wsp:Policy>
   							</wsam:Addressing>
   						</wsp:Policy>
           			 </wsam:Addressing>
        		</wsp:All>
    		</wsp:ExactlyOne>
		</wsp:Policy>
	</wsp:PolicyAttachment>
	
		
	<wsp:PolicyAttachment wsdlNamespace="http://service1.policyattachments.ws.ibm.com/">
		<wsp:AppliesTo>
			<wsp:URI>http://service1.policyattachments.ws.ibm.com/HelloService3/#wsdl11.service(HelloService3)</wsp:URI>
		</wsp:AppliesTo>
		<wsp:Policy wsu:Id="WSAddressing3">
			<wsp:ExactlyOne>
				<wsp:All>
            		<wsam:Addressing>
   						<wsp:Policy xmlns:wsu="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-utility-1.0.xsd" wsu:Id="MixedEndpoint">
   							<wsam:Addressing>
   								<wsp:Policy>
   									<wsam:NonAnonymousResponses required="true"/>
   								</wsp:Policy>
   							</wsam:Addressing>
   						</wsp:Policy>
           			 </wsam:Addressing>
        		</wsp:All>
    		</wsp:ExactlyOne>
        </wsp:Policy>
	</wsp:PolicyAttachment>
	
	<wsp:PolicyAttachment wsdlNamespace="http://service1.policyattachments.ws.ibm.com/">
		<wsp:AppliesTo>
			<wsp:URI>http://service1.policyattachments.ws.ibm.com/HelloService4/#wsdl11.service(HelloService4)</wsp:URI>
		</wsp:AppliesTo>
		<wsp:Policy wsu:Id="WSAddressing4">
			<wsp:ExactlyOne>
				<wsp:All>
            		<wsam:Addressing>
   						<wsp:Policy xmlns:wsu="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-utility-1.0.xsd" wsu:Id="MixedEndpoint2">
   							<wsam:Addressing>
   								<wsp:Policy>
   									<wsam:AnonymousResponses required="true"/>
   								</wsp:Policy>
   							</wsam:Addressing>
   						</wsp:Policy>
           			 </wsam:Addressing>
        		</wsp:All>
    		</wsp:ExactlyOne>
    	</wsp:Policy>
	</wsp:PolicyAttachment>
</attachments>