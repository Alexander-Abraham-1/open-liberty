-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Bundle-Name: MP GraphQL SmallRye Metrics Implementation
Bundle-SymbolicName: com.ibm.ws.io.smallrye.graphql.metrics
Bundle-Description: MP GraphQL Smallrye Metrics Collection Implementation


Export-Package: \
  com.ibm.ws.io.smallrye.graphql.metrics.component;thread-context=true,\
  io.smallrye.graphql.*;version=1.0.1;thread-context=true

Include-Resource: \
  META-INF=resources/META-INF

src: src/

-cdiannotations: 

-dsannotations: com.ibm.ws.io.smallrye.graphql.metrics.component.*

-includeresource: \
  @${repo;io.smallrye:smallrye-graphql-metrics;1.0.1.20200330}!/!META-INF/beans.xml,\
  com/ibm/ws=${bin}/com/ibm/ws,\
  META-INF=resources/META-INF

-buildpath: \
  io.smallrye:smallrye-graphql-metrics;strategy=exact;version=1.0.1.20200330,\
  com.ibm.websphere.javaee.annotation.1.3;version=latest, \
  com.ibm.websphere.javaee.cdi.2.0;version=latest,\
  com.ibm.websphere.javaee.interceptor.1.2;version=latest,\
  com.ibm.websphere.org.eclipse.microprofile.graphql.1.0;version=latest,\
  com.ibm.websphere.org.eclipse.microprofile.metrics.2.3;version=latest,\
  com.ibm.wsspi.org.osgi.service.component.annotations;version=latest,\
  com.ibm.ws.cdi.interfaces;version=latest,\
  com.ibm.ws.logging;version=latest,\
  com.ibm.ws.microprofile.metrics.cdi.2.3;version=latest,\
